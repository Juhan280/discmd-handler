{
  "name": "discmd-handler",
  "version": "1.0.0",
  "description": "discmd-handler is a lightweight and flexible command handler for Discord.js bots that simplifies the process of creating and managing commands.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup-node",
    "lint": "tsc && eslint src",
    "format": "prettier -w .",
    "update": "changeset version && typedoc",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "discord",
    "command",
    "handler",
    "discord.js",
    "djs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juhan280/discmd-handler.git"
  },
  "author": "Juhan280",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Juhan280/discmd-handler/issues"
  },
  "homepage": "https://Juhan280.github.io/discmd-handler",
  "dependencies": {
    "chalk": "^4.1.2",
    "fast-glob": "^3.2.12",
    "normalize-path": "^3.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/node": "^18.15.10",
    "@types/normalize-path": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "discord.js": "^14.8.0",
    "eslint": "^8.36.0",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tsup": "^6.7.0",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "discord.js": "^14.8.0"
  }
}
